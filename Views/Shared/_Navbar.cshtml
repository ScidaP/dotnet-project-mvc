@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor

@if (String.IsNullOrEmpty(HttpContextAccessor.HttpContext.Session.GetString("Rol"))) {
    @Html.Partial("_NavbarNoLogueado");
} else {
    @if (HttpContextAccessor.HttpContext.Session.GetInt32("Rol") == 1) {
        @Html.Partial("_NavbarAdmin");
    } else {
        @Html.Partial("_NavbarSupervisor");
    }
}